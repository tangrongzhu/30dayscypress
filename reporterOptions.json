{
    "reporterEnabled": "mocha-junit-reporter, mochawesome, spec", 
    "mochaJunitReporterReporterOptions": {
      "mochaFile": "cypress/reports/junit/test_results[hash].xml",
      "toConsole": true
    },
    "mochawesomeReporterOptions": {
      "reportDir": "cypress/reports/mocha", 
      "quiet": true, 
      "overwrite": false, 
      "html": false,
      "json": true
  
    }
}